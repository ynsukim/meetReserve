{
  "expo": {
    "name": "meetReserve",
    "slug": "meetReserve",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./src/assets/images/app-icon/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meetreserve.app",
      "screenOrientation": "landscape"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app-icon/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.meetreserve.app",
      "screenOrientation": "landscape"
    },
    "web": {
      "favicon": "./src/assets/images/app-icon/favicon.png"
    },
    "scheme": "meetreserve",
    "plugins": [
      "expo-router"
    ]
  }
}
