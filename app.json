{
  "expo": {
    "name": "meetReserve",
    "slug": "meetReserve",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/appRelated/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/appRelated/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meetreserve.app",
      "statusBar": {
        "backgroundColor": "#000000",
        "barStyle": "dark-content"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appRelated/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.meetreserve.app",
      "statusBar": {
        "backgroundColor": "#000000",
        "barStyle": "dark-content"
      }
    },
    "web": {
      "favicon": "./assets/appRelated/favicon.png"
    },
    "scheme": "meetreserve",
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ]
  }
}
